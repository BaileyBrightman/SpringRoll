{
  "name": "springroll",
  "version": "2.0.0",
  "description": "",
  "module": "dist/Springroll.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run readme",
    "build:map": "rollup -c -m",
    "build:full": "npm run build:map && npm run docs && npm run readme",
    "dev": "rollup -c -w",
    "test": "karma start karma.conf.js",
    "docs": "documentation build ./src/index.js -f html -o docs",
    "readme": "documentation build ./src/index.js -f md -o docs.md",
    "docs:serve": "documentation serve --watch ./src/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "documentation": "^8.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "karma": "^2.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-requirejs": "^1.1.0",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "requirejs": "^2.3.5",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "webpack": "^4.10.2"
  },
  "dependencies": {
    "bellhop-iframe": "^2.1.1"
  }
}
